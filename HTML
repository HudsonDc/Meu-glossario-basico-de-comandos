<!-- Estrutura básica -->
<html>        <!-- Início do documento HTML -->
<head>        <!-- Metadados, título, links de CSS -->
<title>       <!-- Título da aba do navegador -->
<body>        <!-- Conteúdo visível da página -->

<!-- Títulos -->
<h1>          <!-- Título principal -->
<h2>          <!-- Subtítulo -->
<h3>          <!-- Sub-subtítulo -->
<h4>          <!-- Menor que h3 -->
<h5>          <!-- Menor que h4 -->
<h6>          <!-- Menor que h5 -->

<!-- Texto e links -->
<p>           <!-- Parágrafo -->
<a href="#">  <!-- Link clicável -->
<br>          <!-- Quebra de linha -->
<hr>          <!-- Linha horizontal -->

<!-- Imagens -->
<img src="imagem.jpg" alt="Descrição"> <!-- Imagem -->

<!-- Listas -->
<ul>          <!-- Lista não ordenada -->
<ol>          <!-- Lista ordenada -->
<li>          <!-- Item da lista -->

<!-- Estrutura semântica -->
<header>      <!-- Cabeçalho -->
<footer>      <!-- Rodapé -->
<nav>         <!-- Menu de navegação -->
<main>        <!-- Conteúdo principal -->
<section>     <!-- Seção temática -->
<article>     <!-- Conteúdo independente -->

<!-- Containers -->
<div>         <!-- Bloco genérico -->
<span>        <!-- Texto inline -->

<!-- Formulários -->
<form>        <!-- Formulário -->
<input type="text"> <!-- Campo de entrada -->
<button>      <!-- Botão -->